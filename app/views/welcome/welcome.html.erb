<body id="welcome">
  <h1>Formulary One</h1>
  <h2>Request Access Token</h2>
  <div id="signUpForm">
    <%= form_for :user, url: 'users' do |f| %>
      <h3>Email: <%= f.text_field :email %>
      <br>
      <br>
      Password: <%= f.password_field :password %>
      <%= f.submit "Submit" %></h3>
    <% end %>
  <h1><%#= link_to "See Users", 'users' %></h1>
</body>